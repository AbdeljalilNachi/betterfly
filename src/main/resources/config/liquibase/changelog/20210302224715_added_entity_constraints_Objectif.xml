<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Objectif.
    -->
    <changeSet id="20210302224715-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="action_id"
                                 baseTableName="objectif"
                                 constraintName="fk_objectif_action_id"
                                 referencedColumnNames="id"
                                 referencedTableName="action"/>

        <addForeignKeyConstraint baseColumnNames="delegue_id"
                                 baseTableName="objectif"
                                 constraintName="fk_objectif_delegue_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="processus_id"
                                 baseTableName="objectif"
                                 constraintName="fk_objectif_processus_id"
                                 referencedColumnNames="id"
                                 referencedTableName="processus_smi"/>

        <addForeignKeyConstraint baseColumnNames="indicateur_id"
                                 baseTableName="objectif"
                                 constraintName="fk_objectif_indicateur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="indicateur_smi"/>

    </changeSet>
</databaseChangeLog>
