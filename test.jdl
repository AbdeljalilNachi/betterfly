  situation Situation,
  frequence EnumFive,
  sensibiliteMilieu EnumFive,
  coefficientMaitrise EnumFive,
  gravite EnumFive,
  criticite Integer,
  maitriseExistante String,
  origine String
}
entity AnalyseSST {
